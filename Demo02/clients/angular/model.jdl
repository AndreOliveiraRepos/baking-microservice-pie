
entity Book {
    title String required,
    author String,
    year Integer,
    genre Genre,
    isbn String
}

enum Genre {
  JAZZ, ROCK, FOLK, POP, BLUES, HIP_HOP, RAPPING, COUNTRY, PUNK_ROCK, RHYTHM_AND_BLUES, ELECTRONIC_DANCE, SINGING, REGGAE, POPULAR, DISCO, FUNK, SOUL, TECHNO, CLASSICAL_PERIOD,
  ALTERNATIVE_ROCK, CLASSICAL, INSTRUMENTAL, HOUSE, DUBSTEP
}
